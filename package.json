{
  "name": "decode-vscode",
  "displayName": "decode-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "publisher": "your-publisher-name",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "decodeVscodeContainer",
          "title": "Decode VSCode",
          "icon": "assets/binary.png"
        }
      ]
    },
    "views": {
      "decodeVscodeContainer": [
        {
          "id": "decode-vs-code",
          "name": "Decode VSCode View"
        }
      ]
    },
    "commands": [
      {
        "command": "decode-vs-code.openChatDetails",
        "title": "Open Chat Details"
      },
      {
        "command": "decode-vs-code.createChatSession",
        "title": "Create Chat Session"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/babel__traverse": "^7.20.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.196",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "babel-loader": "^9.1.3",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/parser": "^7.22.10",
    "@babel/traverse": "^7.22.10",
    "abort-controller": "^3.0.0",
    "dotenv": "^16.3.1",
    "downshift": "^8.1.0",
    "lodash": "^4.17.21",
    "mongodb-client-encryption": "^2.9.0",
    "mongoose": "^7.4.3",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.14.2",
    "react-syntax-highlighter": "^15.5.0",
    "realm": "^11.10.2",
    "realm-web": "^2.0.0",
    "uuid": "^9.0.0"
  }
}
